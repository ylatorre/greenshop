{% extends 'base.html.twig' %}

{% block title %}VarianteProduit index{% endblock %}

{% block body %}
    <h1>VarianteProduit index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for variante_produit in variante_produits %}
            <tr>
                <td>{{ variante_produit.id }}</td>
                <td>
                    <a href="{{ path('app_variante_produit_show', {'id': variante_produit.id}) }}">show</a>
                    <a href="{{ path('app_variante_produit_edit', {'id': variante_produit.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_variante_produit_new') }}">Create new</a>
{% endblock %}
